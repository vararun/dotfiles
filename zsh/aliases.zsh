# git aliases
alias gs="git status"
alias gp="git pull"
alias gc="git commit -m"
alias gb="git branch"
alias ga="git add -A"
alias gp="git push"
alias gpf="git push --force"
alias gr="git reset"
alias gac="git add -A && git commit -m"

